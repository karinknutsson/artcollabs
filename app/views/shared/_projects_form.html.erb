<%= simple_form_for @project do |f| %>
  <%= f.input :title, input_html: { class: "artcollabs-input", maxlength: 56 } %>
  <%= f.input :description, input_html: { class: "artcollabs-input bio", maxlength: 10000 } %>
  <%= f.input :tag_list, label: "Tags", input_html: { class: "artcollabs-input" } %>
  <%= f.input :max_members, input_html: { class: "artcollabs-input", maxlength: 15 } %>
  <%= f.input :start_date, input_html: { class: "artcollabs-input" } %>
  <%= f.input :end_date, input_html: { class: "artcollabs-input" } %>
  <%= f.input :photo %>
  <div class="float-right">
    <%= f.submit %>
  </div>
<% end %>
